{
  "private": false,
  "name": "wix-ui-framework",
  "bin": {
    "wuf": "./bin/wuf.js"
  },
  "version": "3.7.2",
  "author": {
    "name": "Wix",
    "email": "fed-infra@wix.com"
  },
  "files": [
    "dist",
    "src",
    "plugins",
    "bin",
    "*.md",
    "*.json"
  ],
  "bugs": {
    "url": "https://github.com/wix/wix-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wix/wix-ui.git"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "cp -r dist/cli-commands/make/plugins plugins",
    "pretest": "npm run lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "postlint": "node scripts/make-readme.js > README.md",
    "start": "tsc --watch",
    "wuf": "./bin/wuf.js"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "commander": "^8.1.0",
    "dependency-tree": "^8.0.0",
    "ejs": "^2.6.2",
    "fs-extra": "^10.0.0",
    "jscodeshift": "^0.6.4",
    "lodash": "^4.17.11",
    "minimatch": "^3.0.4",
    "mkdirp": "^0.5.1",
    "ora": "^3.4.0",
    "prompts": "^2.0.4",
    "react-autodocs-utils": "^3.7.1",
    "simple-git": "^1.110.0"
  },
  "jest": {
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/test-generated/",
      "<rootDir>/src/cli-commands/generate/__testfixtures__"
    ]
  },
  "devDependencies": {
    "@types/dependency-tree": "^8.0.0",
    "@types/jest": "^26.0.24",
    "@types/minimatch": "^3.0.5",
    "@types/node": "^14.17.6",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "cista": "0.0.3",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "nixt": "^0.5.1",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  }
}
