{
  "name": "wix-storybook-utils",
  "version": "4.0.0",
  "description": "Utilities for automated component documentation within Storybook",
  "author": {
    "name": "Wix",
    "email": "fed-infra@wix.com"
  },
  "license": "MIT",
  "files": [
    "src",
    "dist",
    "docs",
    "*.js"
  ],
  "bugs": {
    "url": "https://github.com/wix/wix-ui/issues"
  },
  "homepage": "https://github.com/wix/wix-ui#readme",
  "main": "./dist/src/index.js",
  "module": "./dist/es/src/index.js",
  "scripts": {
    "lint": "yoshi lint",
    "build": "yoshi build && import-path --path src",
    "postbuild": "npm run docs:sections",
    "pretest": "npm run lint",
    "test": "npm run test:unit && npm run test:visual",
    "posttest": "SKIP_THREAD_LOADER=true build-storybook -c ./.storybook/docs && npx janet deploy --ci",
    "test:unit": "jest",
    "test:visual": "SKIP_THREAD_LOADER=true eyes-storybook -c ./.storybook/visual -f .storybook/visual/applitools.config.js",
    "test:watch": "jest --watch",
    "storybook:visual": "SKIP_THREAD_LOADER=true start-storybook -c ./.storybook/visual -p 6007",
    "start": "SKIP_THREAD_LOADER=true start-storybook -p 6006 -c ./.storybook/docs",
    "release": "yoshi release",
    "docs:sections": "node docs/make-sections-docs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wix/wix-ui.git"
  },
  "keywords": [
    "storybook",
    "utilities",
    "documentation",
    "generated",
    "automatic",
    "react"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "@storybook/addons": "^5.0.0"
  },
  "dependencies": {
    "@babel/generator": "^7.11.0",
    "@babel/parser": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/standalone": "^7.13.14",
    "@babel/traverse": "^7.11.0",
    "@babel/types": "^7.11.0",
    "classnames": "^2.2.5",
    "codemirror": "^5.60.0",
    "copy-to-clipboard": "^3.0.8",
    "downshift": "^3.1.4",
    "ejs": "^2.7.1",
    "github-markdown-css": "^2.9.0",
    "highlight.js": "9.14.2",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.15",
    "parse-prop-types": "^0.3.0",
    "prettier": "^1.16.2",
    "query-string": "^6.0.0",
    "react-autodocs-utils": "^3.5.0",
    "react-codemirror2": "^7.2.1",
    "react-collapse": "^4.0.3",
    "react-element-to-jsx-string": "^14.0.2",
    "react-live": "^2.2.1",
    "react-motion": "^0.5.2",
    "react-remarkable": "^1.1.3",
    "react-syntax-highlighter": "^9.0.0",
    "react-tooltip": "4.2.13",
    "webpack-inject-plugin": "^1.5.5",
    "wix-ui-icons-common": "^2.0.311"
  },
  "devDependencies": {
    "@applitools/eyes-storybook": "^3.25.1",
    "@mdx-js/loader": "^0.17.3",
    "@storybook/addon-options": "^5.2.4",
    "@storybook/addons": "^5.0.0",
    "@storybook/react": "^5.2.4",
    "@stylable/jest": "^3.13.0",
    "@stylable/runtime": "^3.13.0",
    "@stylable/webpack-plugin": "^3.13.0",
    "@testing-library/react": "^11.2.5",
    "@types/codemirror": "0.0.108",
    "@types/jest": "^23.3.8",
    "@types/node": "^12.12.22",
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.8.1",
    "@wix/yoshi": "^4.16.4",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.5",
    "identity-obj-proxy": "^3.0.0",
    "import-path": "latest",
    "node-sass": "^4.9.4",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-test-renderer": "^16.4.2",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "storybook-snapper": "^1.0.67",
    "ts-jest": "^24.2.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.1.3",
    "wix-ui-test-utils": "^1.0.120",
    "yoshi-style-dependencies": "^4.11.1"
  }
}
